{
	"bsonType": "object",
	"required": ["start_time","startType","end_time","user_id"],
	"permission": {
		"read": "auth.uid != null",
		"create": "auth.uid != null",
		"update": "auth.uid != null",
		"delete": "auth.uid != null"
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"start_time":{
			"bsonType":"timestamp"
		},
		"startType":{
			"bsonType": "int",
			"title": "类型",
			"defaultValue":0,
			"enum":[
				{
					"text":"公历",
					"value":0
				},
				{
					"text":"农历",
					"value":1
				}
			]
		},
		"end_time":{
			"bsonType":"timestamp"
		},
		"user_id":{
			"bsonType": "string",
			"forceDefaultValue":{
				"$env":"uid"
			}
		}
	}
}
